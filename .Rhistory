#Creation d'une fonction
```{r fonction}
freg=function(data){
regress<-summary(lm(y ~.,data=data))
estimate<-regress$coefficients[,1]
tvalue<-regress$coefficients[,3]
return(data.frame(estimate,tvalue))
}
freg=function(data){
regress<-summary(lm(y ~.,data=data))
estimate<-regress$coefficients[,1]
tvalue<-regress$coefficients[,3]
return(data.frame(estimate,tvalue))
}
